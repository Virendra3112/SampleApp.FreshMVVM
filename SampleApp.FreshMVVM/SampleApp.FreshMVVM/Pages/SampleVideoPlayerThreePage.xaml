<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SampleApp.FreshMVVM.Pages.SampleVideoPlayerThreePage"
             xmlns:roxv="clr-namespace:Rox;assembly=Rox.Xamarin.Video.Portable">
    <ContentPage.Content>
        <StackLayout BackgroundColor="Yellow" HeightRequest="200" WidthRequest="200" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">
            <roxv:VideoView BackgroundColor="Pink" AutoPlay="True" LoopPlay="True" ShowController="True"
                            Source="http://commondatastorage.googleapis.com/gtv-videos-bucket/sample/ElephantsDream.mp4" />
            
        </StackLayout>
</ContentPage.Content>
</ContentPage>